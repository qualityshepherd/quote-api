{
  "name": "quote-api",
  "author": "brine",
  "type": "module",
  "version": "1.0.0",
  "description": "a simple--if not silly--movie quote api",
  "main": "app.js",
  "scripts": {
    "start": "pm2 start babel-node app.js",
    "restart": "pm2 restart",
    "stop": "pm2 stop 0 1",
    "watch": "nodemon babel-node app.js",
    "test": "tape tests.js",
    "test:watch": "nodemon tape tests.js",
    "test:watch:prod": "BASEURL=https://api.brine.dev nodemon tape tests.js",
    "test:prod": "BASEURL=https://api.brine.dev npm test",
    "pretty": "standard --fix"
  },
  "dependencies": {
    "@babel/core": "^7.22.5",
    "@babel/node": "^7.22.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-proposal-throw-expressions": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "express": "^4.18.2",
    "node-fetch": "^3.3.1",
    "nodemon": "^2.0.22",
    "pm2": "^5.3.0",
    "standard": "^17.1.0",
    "tape": "^5.6.4"
  }
}
